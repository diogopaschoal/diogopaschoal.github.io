    "marked": "^9.1.6",
    "patch-package": "^8.0.1",
    "webpack": "^5.104.1"
  },
  "overrides": {
    "braces": "3.0.3",
    "micromatch": "4.0.8",
    "anymatch": "3.1.3",
    "chokidar": "3.5.3",
    "readdirp": "3.6.0",
    "watchpack": "2.5.1",
    "node-libs-browser": "2.2.1",
    "cross-spawn": "7.0.6",
    "execa": "5.1.1"
